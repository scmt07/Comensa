<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.example.tulio.tcc">
    
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:name=".BD.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".SplashActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".CadastroActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".ListaEstabActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".DetalhesActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".TrocarSenhaActivity"
            android:theme="@style/AppTheme"></activity>

    </application>

</manifest>
